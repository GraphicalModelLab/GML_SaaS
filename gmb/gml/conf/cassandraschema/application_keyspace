CREATE KEYSPACE master WITH REPLICATION = { 'class' : 'SimpleStrategy', 'replication_factor' : 1 };
                                                                                                                                                                                                                    CREATE TABLE master.model (companyid varchar,userid varchar, modelid varchar, datetime timestamp, model varchar, PRIMARY KEY(companyid,userid,modelid));
                                                                                                                                                                                                                    CREATE TABLE master.model_training_history (companyid varchar,userid varchar, modelid varchar, datetime timestamp, model varchar, training_data varchar, PRIMARY KEY(companyid,userid,modelid, datetime));
                                                                                                                                                                                                                    CREATE TABLE master.model_test_history (companyid varchar,userid varchar, modelid varchar, datetime timestamp, model varchar, test_data varchar, accuracy varchar, PRIMARY KEY(companyid,userid,modelid, datetime));